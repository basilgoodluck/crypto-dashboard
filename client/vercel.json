{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "/"
      },
      {
        "src": "/dashboard",
        "dest": "/"
      },
      {
        "src": "/sign-in",
        "dest": "/"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }